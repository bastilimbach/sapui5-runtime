{
    "extends": "airbnb-base",
    "env": {
        "node": true
    },
    "rules": {
        "semi": [
            "error",
            "never"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "no-console": "off",
        "import/no-dynamic-require": "off"
    }
}
